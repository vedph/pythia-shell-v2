{
  "name": "pythia-shell",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build-cor": "ng build @myrmidon/pythia-core",
    "build-api": "ng build @myrmidon/pythia-api",
    "build-ui": "ng build @myrmidon/pythia-ui",
    "build-dcr": "ng build @myrmidon/pythia-document-reader",
    "build-doc": "ng build @myrmidon/pythia-document-list",
    "build-wrd": "ng build @myrmidon/pythia-word-index",
    "build-sta": "ng build @myrmidon/pythia-stats",
    "build-qbl": "ng build @myrmidon/pythia-query-builder",
    "build-src": "ng build @myrmidon/pythia-search",
    "build-crp": "ng build @myrmidon/pythia-corpus-list",
    "build-lib": "pnpm run build-cor && pnpm run build-api && pnpm run build-ui && pnpm run build-dcr && pnpm run build-doc && pnpm run build-wrd && pnpm run build-sta && pnpm run build-qbl && pnpm run build-src && pnpm run build-crp && pnpm run build-qbl",
    "xi18n": "ng extract-i18n --output-path src/locale && xliffmerge --profile xliffmerge.json it"
  },
  "private": true,
  "packageManager": "pnpm@10.23.0",
  "dependencies": {
    "@angular/animations": "^21.0.8",
    "@angular/cdk": "^21.0.6",
    "@angular/common": "^21.0.8",
    "@angular/compiler": "^21.0.8",
    "@angular/core": "^21.0.8",
    "@angular/forms": "^21.0.8",
    "@angular/material": "^21.0.6",
    "@angular/platform-browser": "^21.0.8",
    "@angular/platform-browser-dynamic": "^21.0.8",
    "@angular/router": "^21.0.8",
    "@myrmidon/auth-jwt-admin": "^8.0.3",
    "@myrmidon/auth-jwt-login": "^8.0.3",
    "@myrmidon/cadmus-refs-lookup": "^10.0.6",
    "@myrmidon/ngx-mat-tools": "^1.0.1",
    "@myrmidon/ngx-tools": "^2.0.2",
    "@myrmidon/paged-data-browsers": "^5.1.2",
    "echarts": "^6.0.0",
    "ngx-echarts": "^21.0.0",
    "rxjs": "~7.8.0",
    "ts-md5": "^2.0.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.5",
    "@angular/cli": "^21.0.5",
    "@angular/compiler-cli": "^21.0.8",
    "@angular/localize": "^21.0.8",
    "@types/jasmine": "~5.1.13",
    "jasmine-core": "~5.13.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^21.0.1",
    "ngx-i18nsupport": "^0.17.1",
    "typescript": "~5.9.3"
  },
  "optionalDependencies": {
    "@rollup/rollup-win32-arm64-msvc": "^4.55.1"
  },
  "pnpm": {
    "overrides": {
      "rollup": "npm:@rollup/wasm-node"
    }
  }
}
