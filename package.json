{
  "name": "pythia-shell",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build-cor": "ng build @myrmidon/pythia-core",
    "build-api": "ng build @myrmidon/pythia-api",
    "build-ui": "ng build @myrmidon/pythia-ui",
    "build-dcr": "ng build @myrmidon/pythia-document-reader",
    "build-doc": "ng build @myrmidon/pythia-document-list",
    "build-wrd": "ng build @myrmidon/pythia-word-index",
    "build-sta": "ng build @myrmidon/pythia-stats",
    "build-qbl": "ng build @myrmidon/pythia-query-builder",
    "build-src": "ng build @myrmidon/pythia-search",
    "build-crp": "ng build @myrmidon/pythia-corpus-list",
    "build-lib": "npm run build-cor && npm run build-api && npm run build-ui && npm run build-dcr && npm run build-doc && npm run build-wrd && npm run build-sta && npm run build-qbl && npm run build-src && npm run build-crp && npm run build-qbl",
    "xi18n": "ng extract-i18n --output-path src/locale && xliffmerge --profile xliffmerge.json it"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.3.0",
    "@angular/cdk": "^20.2.3",
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.0",
    "@angular/material": "^20.2.3",
    "@angular/platform-browser": "^20.3.0",
    "@angular/platform-browser-dynamic": "^20.3.0",
    "@angular/router": "^20.3.0",
    "@myrmidon/auth-jwt-admin": "^8.0.2",
    "@myrmidon/auth-jwt-login": "^8.0.2",
    "@myrmidon/cadmus-refs-lookup": "^10.0.3",
    "@myrmidon/ngx-mat-tools": "^1.0.0",
    "@myrmidon/ngx-tools": "^2.0.0",
    "@myrmidon/paged-data-browsers": "^5.0.2",
    "echarts": "^6.0.0",
    "ngx-echarts": "^20.0.2",
    "rxjs": "~7.8.0",
    "ts-md5": "^2.0.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.1",
    "@angular/cli": "^20.3.1",
    "@angular/compiler-cli": "^20.3.0",
    "@angular/localize": "^20.3.0",
    "@types/jasmine": "~5.1.9",
    "jasmine-core": "~5.10.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^20.3.0",
    "ngx-i18nsupport": "^0.17.1",
    "typescript": "~5.9.2"
  }
}
